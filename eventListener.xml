<?xml version="1.0" encoding="utf-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/eventListener.xsd">
    <import>
		<eventlistener name="userGroupAddMinecraftAdmin">
			<eventclassname>wcf\acp\form\UserGroupAddForm</eventclassname>
			<eventname>readFormParameters,validate,save,assignVariables</eventname>
			<environment>admin</environment>
			<inherit>1</inherit>
			<listenerclassname>wcf\system\event\listener\MinecraftAcpUserGroupAddListener</listenerclassname>
		</eventlistener>
		
		<eventlistener name="userAddMinecraftAdmin">
			<eventclassname>wcf\acp\form\UserEditForm</eventclassname>
			<eventname>assignVariables</eventname>
			<environment>admin</environment>
			<inherit>1</inherit>
			<listenerclassname>wcf\system\event\listener\MinecraftAcpUserEditListener</listenerclassname>
		</eventlistener>
		
		<eventlistener name="minecraftSyncAdmin">
			<eventclassname>wcf\data\user\UserAction</eventclassname>
			<eventname>initializeAction,finalizeAction</eventname>
			<inherit>1</inherit>
			<environment>admin</environment>
			<listenerclassname>wcf\system\event\listener\MinecraftSyncListener</listenerclassname>
		</eventlistener>
		
		<eventlistener name="gdprExportAdmin">
			<eventclassname>wcf\acp\action\UserExportGdprAction</eventclassname>
			<eventname>export</eventname>
			<listenerclassname>wcf\system\event\listener\MinecraftSyncGdprExportListener</listenerclassname>
			<environment>admin</environment>
			<inherit>0</inherit>
		</eventlistener>
		
		<eventlistener name="minecraftSyncFrontend">
			<eventclassname>wcf\data\user\UserAction</eventclassname>
			<eventname>initializeAction,finalizeAction</eventname>
			<inherit>1</inherit>
			<environment>user</environment>
			<listenerclassname>wcf\system\event\listener\MinecraftSyncListener</listenerclassname>
		</eventlistener>

		<eventlistener name="minecraftMandatoryCheckPage">
			<eventclassname>wcf\page\AbstractPage</eventclassname>
			<eventname>readParameters</eventname>
			<listenerclassname>wcf\system\event\listener\MinecraftMandatoryListener</listenerclassname>
			<inherit>1</inherit>
		</eventlistener>
    </import>
</data>
