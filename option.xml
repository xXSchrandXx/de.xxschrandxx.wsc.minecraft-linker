<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="https://www.woltlab.com"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/5.5/option.xsd">
	<import>
		<categories>
			<category name="minecraft_linker">
				<parent>minecraft</parent>
			</category>
			<category name="minecraft_linker_general">
				<parent>minecraft_linker</parent>
			</category>
			<category name="minecraft_linker_password">
				<parent>minecraft_linker</parent>
			</category>
		</categories>
		<options>
			<option name="minecraft_linker_enabled">
				<categoryname>minecraft_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>minecraft_linker_identity,minecraft_max_uuids,minecraft_name_enabled,minecraft_enable_active_user,minecraft_password_page</enableoptions>
			</option>
			<option name="minecraft_linker_identity">
				<categoryname>minecraft_linker_general</categoryname>
				<optiontype>MinecraftConnectionMultiSelect</optiontype>
			</option>
			<option name="minecraft_max_uuids">
				<categoryname>minecraft_linker_general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<suffix>mc_uuids</suffix>
			</option>
			<option name="minecraft_name_enabled">
				<categoryname>minecraft_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="minecraft_enable_active_user">
				<categoryname>minecraft_linker_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
		</options>
	</import>
	<delete>
		<option name="minecraft_command_list" />
		<option name="minecraft_command_sendcode" />
		<optioncategory name="minecraft_linker_commands" />
	</delete>
</data>
