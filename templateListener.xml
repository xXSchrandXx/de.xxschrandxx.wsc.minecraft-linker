<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/templateListener.xsd">
	<import>
		<!-- admin -->
		<templatelistener name="userGroupAddSection">
			<environment>admin</environment>
			<templatename>userGroupAdd</templatename>
			<eventname>sections</eventname>
			<templatecode><![CDATA[{include file='__minecraftUserGroupAddSection'}]]></templatecode>
		</templatelistener>
		
		<templatelistener name="userAddSection">
			<environment>admin</environment>
			<templatename>userAdd</templatename>
			<eventname>sections</eventname>
			<templatecode><![CDATA[{include file='__minecraftUserAddSection'}]]></templatecode>
		</templatelistener>
		
		<templatelistener name="userListColumnHeads">
			<environment>admin</environment>
			<templatename>userList</templatename>
			<eventname>columnHeads</eventname>
			<templatecode><![CDATA[<th></th>]]></templatecode>
		</templatelistener>
		
		<templatelistener name="userListColumns">
			<environment>admin</environment>
			<templatename>userList</templatename>
			<eventname>columns</eventname>
			<templatecode><![CDATA[{include file='__minecraftUserListIdentity'}]]></templatecode>
		</templatelistener>
		<!-- /admin -->
	</import>
</data>
